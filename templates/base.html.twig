<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}ArtDome{% endblock %}</title>
    {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
    {% block stylesheets %}
        <link href="{{ asset('Front/css/bootstrap.min.css') }}" rel="stylesheet">
        <link href="{{ asset('Front/https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css') }}" rel="stylesheet">
        <link href="{{ asset('Front/css/style.css') }}" rel="stylesheet">
        <link href="{{ asset('Front/https://fonts.googleapis.com/css?family=Dosis:200,300,400,500,600,700') }}" rel="stylesheet">
        <link href="{{ asset('Front/https://fonts.googleapis.com/css?family=Roboto:200,300,400,500,600,700') }}" rel="stylesheet">
    {% endblock %}


</head>
<body>
<header class="item header margin-top-0">
    <div class="wrapper">
        <nav role="navigation" class="navbar navbar-white navbar-embossed navbar-lg navbar-fixed-top">
            <div class="container">
                <div class="navbar-header">
                    <button data-target="#navbar-collapse-02" data-toggle="collapse" class="navbar-toggle" type="button">
                        <i class="fa fa-bars"></i>
                        <span class="sr-only">Toggle navigation</span>
                    </button>
                    <a href="{{ asset('index.html') }}" class="navbar-brand brand">
                        <img width="85" height="50" src="{{ asset('../Front/GFX/a.png') }}" alt="">
                    </a>
                </div>
                <div id="navbar-collapse-02" class="collapse navbar-collapse">
                    <ul class="nav navbar-nav navbar-right">
                        <li class="propClone"><a href="{{ asset('index.html') }}">Home</a></li>
                        <li class="propClone"><a href="{{ path('reservation_expo_index') }}">Exposition Reservations</a></li>
                        <li class="propClone"><a href="{{ path('exposition_index') }}">Expositions</a></li>

                    </ul>
                </div>
            </div>
        </nav>
        <div class="container">
            <div class="row">
                <div class="col-md-12 text-center">
                    <div class="text-homeimage">
                        <div class="maintext-image" data-scrollreveal="enter top over 1.5s after 0.1s">
                            Increase Digital Sales
                        </div>
                        <div class="subtext-image" data-scrollreveal="enter bottom over 1.7s after 0.3s">
                            Boost revenue with ArtDome
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</header>
{% block body %}
    <!-- HEADER =============================-->


{% endblock %}
{% block javascripts %}
    <!-- SCRIPTS =============================-->
    <script src="{{ asset('Front/js/jquery-.js') }}"></script>
    <script src="{{ asset('Front/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('Front/js/anim.js') }}"></script>
    <script>
        //----HOVER CAPTION---//
        jQuery(document).ready(function ($) {
            $('.fadeshop').hover(
                function(){
                    $(this).find('.captionshop').fadeIn(150);
                },
                function(){
                    $(this).find('.captionshop').fadeOut(150);
                }
            );
        });
    </script>
{% endblock %}
<!-- FOOTER =============================-->
<div class="footer text-center">
    <div class="container">
        <div class="row">
            <p class="footernote">
                Connect with Scorilo
            </p>
            <ul class="social-iconsfooter">
                <li><a href="#"><i class="fa fa-phone"></i></a></li>
                <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                <li><a href="#"><i class="fa fa-google-plus"></i></a></li>
                <li><a href="#"><i class="fa fa-pinterest"></i></a></li>
            </ul>
            <p>
                &copy; 2017 Your Website Name<br/>
                Scorilo - Free template by <a href="https://www.wowthemes.net/">WowThemesNet</a>
            </p>
        </div>
    </div>
</div>
</body>
</html>